  <rd:ReportID>f00a5913-abc3-49b2-b969-1de1128f5403</rd:ReportID>
  <LeftMargin>1in</LeftMargin>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     GrossMarginGroup, AccountGroup, AccountDesc, AccountCode, fiscal_period, DateName(Month, cast(fiscal_period as varchar(2)) + '/1/' + cast(fiscal_year as char(4))) as 'Month',
sum(per_net_amount)  AS Amount
FROM         vIncomeStatement
WHERE     (fiscal_period BETWEEN 1 AND @CurPeriod) AND (fiscal_year = @FiscalYear) AND (entity_num = @Entity)
group by GrossMarginGroup, AccountGroup, AccountDesc, AccountCode, fiscal_period, DateName(Month, cast(fiscal_period as varchar(2)) + '/1/' + cast(fiscal_year as char(4)))
order by  AccountCode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CurPeriod">
            <Value>=Parameters!CurPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FiscalYear">
            <Value>=Parameters!FiscalYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Entity">
            <Value>=Parameters!Entity.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>FinSampleDB</DataSourceName>
      </Query>
      <Filters>
        <Filter>
          <Operator>NotEqual</Operator>
          <FilterValues>
            <FilterValue>Exclude</FilterValue>
          </FilterValues>
          <FilterExpression>=Fields!AccountGroup.Value</FilterExpression>
        </Filter>
      </Filters>
      <Fields>
        <Field Name="GrossMarginGroup">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>GrossMarginGroup</DataField>
        </Field>
        <Field Name="AccountGroup">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AccountGroup</DataField>
        </Field>
        <Field Name="AccountDesc">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AccountDesc</DataField>
        </Field>
        <Field Name="AccountCode">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AccountCode</DataField>
        </Field>
        <Field Name="fiscal_period">
          <rd:TypeName>System.Int16</rd:TypeName>
          <DataField>fiscal_period</DataField>
        </Field>
        <Field Name="Month">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Month</DataField>
        </Field>
        <Field Name="Amount">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Amount</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Entity">
      <Query>
        <CommandText>SELECT     entity_num, entity_desc
FROM         frl_entity</CommandText>
        <DataSourceName>FinSampleDB</DataSourceName>
      </Query>
      <Fields>
        <Field Name="entity_num">
          <rd:TypeName>System.Int16</rd:TypeName>
          <DataField>entity_num</DataField>
        </Field>
        <Field Name="entity_desc">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>entity_desc</DataField>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>

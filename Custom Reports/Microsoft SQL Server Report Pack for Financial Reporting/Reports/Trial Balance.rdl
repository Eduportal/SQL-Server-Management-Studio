  <rd:ReportID>3cbcca32-37fa-4381-b92a-a743cdecbad3</rd:ReportID>
  <LeftMargin>1in</LeftMargin>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select AccountGroupCode, AccountGroupDesc, AccountCode, AccountDesc,
CASE when fiscal_period = @CurPeriod then Amount else 0 END as CurPeriodAmt,
Amount as YTDAmount,
CASE when fiscal_period = @CurPeriod and AccountGroupCode = 4 then Amount * -1
         when fiscal_period = @CurPeriod and AccountGroupCode in (5,6) then Amount else 0 END as PeriodNetIncome,
CASE when AccountGroupCode = 4 then Amount * -1
         when AccountGroupCode in (4,5,6) then Amount else 0 END as YTDNetIncome
from vTrialBalance
where fiscal_year = @FiscalYear and fiscal_period &lt;= @CurPeriod and entity_num = @Entity
order by AccountGroupCode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CurPeriod">
            <Value>=Parameters!CurPeriod.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FiscalYear">
            <Value>=Parameters!FiscalYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Entity">
            <Value>=Parameters!Entity.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>FinSampleDB</DataSourceName>
      </Query>
      <Fields>
        <Field Name="AccountGroupCode">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>AccountGroupCode</DataField>
        </Field>
        <Field Name="AccountGroupDesc">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AccountGroupDesc</DataField>
        </Field>
        <Field Name="AccountCode">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AccountCode</DataField>
        </Field>
        <Field Name="AccountDesc">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>AccountDesc</DataField>
        </Field>
        <Field Name="CurPeriodAmt">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>CurPeriodAmt</DataField>
        </Field>
        <Field Name="YTDAmount">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>YTDAmount</DataField>
        </Field>
        <Field Name="PeriodNetIncome">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>PeriodNetIncome</DataField>
        </Field>
        <Field Name="YTDNetIncome">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>YTDNetIncome</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Entity">
      <Query>
        <CommandText>SELECT     entity_num, entity_desc
FROM         frl_entity</CommandText>
        <DataSourceName>FinSampleDB</DataSourceName>
      </Query>
      <Fields>
        <Field Name="entity_num">
          <rd:TypeName>System.Int16</rd:TypeName>
          <DataField>entity_num</DataField>
        </Field>
        <Field Name="entity_desc">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>entity_desc</DataField>
        </Field>
      </Fields>
    </DataSet>
